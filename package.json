{
  "name": "sprint7",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"npm run build:frontend\" \"npm run build:backend\"",
    "build:frontend": "cd Client && npm install",
    "build:backend": "cd Server && npm install ",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd Client && npm install && npm run dev",
    "start:backend": "cd Server && npm install && npm run build",
    "eslint": "eslint \"./**/*.{js,ts}\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "eslint": "^9.0.0"
  }
}
